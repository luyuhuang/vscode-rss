{
  "name": "rss",
  "displayName": "RSS",
  "description": "A RSS reader embedded in Visual Studio Code",
  "license": "MIT",
  "icon": "logo.png",
  "version": "0.6.1",
  "publisher": "luyuhuang",
  "author": "luyuhuang",
  "homepage": "https://github.com/luyuhuang/vscode-rss.git",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luyuhuang/vscode-rss.git"
  },
  "engines": {
    "vscode": "^1.40.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "news",
    "rss",
    "feed",
    "reader"
  ],
  "activationEvents": [
    "onView:rss-feeds",
    "onView:rss-articles"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "title": "rss",
      "properties": {
        "rss.accounts": {
          "type": "object",
          "default": {},
          "description": "Feed accounts"
        },
        "rss.interval": {
          "type": "integer",
          "default": 3600,
          "description": "Refresh interval(s)"
        },
        "rss.timeout": {
          "type": "integer",
          "default": 15,
          "description": "Request timeout(s)"
        },
        "rss.retry": {
          "type": "integer",
          "default": 1,
          "description": "Request retries"
        },
        "rss.fetch-unread-only": {
          "type": "boolean",
          "default": false,
          "description": "Fetch unread articles only, for TTRSS"
        }
      }
    },
    "commands": [
      {
        "command": "rss.select",
        "title": "Select"
      },
      {
        "command": "rss.new-account",
        "title": "New account",
        "icon": "$(add)"
      },
      {
        "command": "rss.del-account",
        "title": "Delete"
      },
      {
        "command": "rss.account-rename",
        "title": "Rename"
      },
      {
        "command": "rss.account-modify",
        "title": "Modify"
      },
      {
        "command": "rss.articles",
        "title": "Articles"
      },
      {
        "command": "rss.read",
        "title": "Read"
      },
      {
        "command": "rss.refresh",
        "title": "Refresh",
        "icon": "$(refresh)"
      },
      {
        "command": "rss.refresh-account",
        "title": "Refresh",
        "icon": "$(refresh)"
      },
      {
        "command": "rss.refresh-one",
        "title": "Refresh",
        "icon": "$(refresh)"
      },
      {
        "command": "rss.open-website",
        "title": "Open website"
      },
      {
        "command": "rss.open-link",
        "title": "Open link"
      },
      {
        "command": "rss.set-read",
        "title": "Set as read"
      },
      {
        "command": "rss.set-unread",
        "title": "Set as unread"
      },
      {
        "command": "rss.set-all-read",
        "title": "Set all to read",
        "icon": "$(check)"
      },
      {
        "command": "rss.set-account-read",
        "title": "Set all to read",
        "icon": "$(check)"
      },
      {
        "command": "rss.add-feed",
        "title": "Add feed",
        "icon": "$(add)"
      },
      {
        "command": "rss.remove-feed",
        "title": "Remove"
      },
      {
        "command": "rss.add-to-favorites",
        "title": "Add to favorites"
      },
      {
        "command": "rss.remove-from-favorites",
        "title": "Remove from favorites"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "rss-reader",
          "title": "RSS Reader",
          "icon": "resources/rss.svg"
        }
      ]
    },
    "views": {
      "rss-reader": [
        {
          "id": "rss-accounts",
          "name": "Accounts"
        },
        {
          "id": "rss-feeds",
          "name": "Feeds"
        },
        {
          "id": "rss-articles",
          "name": "Articles"
        },
        {
          "id": "rss-favorites",
          "name": "Favorites"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "rss.refresh",
          "when": "view == rss-accounts",
          "group": "navigation"
        },
        {
          "command": "rss.new-account",
          "when": "view == rss-accounts",
          "group": "navigation"
        },
        {
          "command": "rss.refresh-account",
          "when": "view == rss-feeds",
          "group": "navigation"
        },
        {
          "command": "rss.add-feed",
          "when": "view == rss-feeds",
          "group": "navigation"
        },
        {
          "command": "rss.set-account-read",
          "when": "view == rss-feeds",
          "group": "navigation"
        },
        {
          "command": "rss.refresh-one",
          "when": "view == rss-articles",
          "group": "navigation"
        },
        {
          "command": "rss.set-all-read",
          "when": "view == rss-articles",
          "group": "navigation"
        }
      ],
      "view/item/context": [
        {
          "command": "rss.refresh-account",
          "when": "view == rss-accounts",
          "group": "navigation@1"
        },
        {
          "command": "rss.set-account-read",
          "when": "view == rss-accounts",
          "group": "navigation@2"
        },
        {
          "command": "rss.account-rename",
          "when": "view == rss-accounts",
          "group": "navigation@3"
        },
        {
          "command": "rss.account-modify",
          "when": "view == rss-accounts && viewItem != local",
          "group": "navigation@4"
        },
        {
          "command": "rss.del-account",
          "when": "view == rss-accounts",
          "group": "navigation@5"
        },
        {
          "command": "rss.open-link",
          "when": "viewItem == article"
        },
        {
          "command": "rss.set-read",
          "when": "view == rss-articles"
        },
        {
          "command": "rss.set-unread",
          "when": "view == rss-articles"
        },
        {
          "command": "rss.add-to-favorites",
          "when": "view == rss-articles"
        },
        {
          "command": "rss.remove-from-favorites",
          "when": "view == rss-favorites && viewItem == article"
        },
        {
          "command": "rss.set-all-read",
          "when": "viewItem == feed"
        },
        {
          "command": "rss.refresh-one",
          "when": "viewItem == feed"
        },
        {
          "command": "rss.open-website",
          "when": "viewItem == feed"
        },
        {
          "command": "rss.remove-feed",
          "when": "viewItem == feed"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile-release",
    "compile-release": "rm -rf ./out && webpack --mode production",
    "compile": "rm -rf ./out && tsc -p ./",
    "lint": "eslint src --ext ts",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/cheerio": "0.22.16",
    "@types/glob": "^7.1.1",
    "@types/he": "^1.1.0",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.0",
    "@types/uuid": "^7.0.3",
    "@types/vscode": "^1.40.0",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "eslint": "^6.8.0",
    "glob": "^7.1.6",
    "mocha": "^7.1.1",
    "ts-loader": "^7.0.5",
    "typescript": "^3.8.3",
    "vscode-test": "^1.3.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {
    "cheerio": "1.0.0-rc.3",
    "fast-xml-parser": "^3.16.0",
    "got": "10.6.0",
    "he": "^1.2.0",
    "iconv-lite": "^0.5.1",
    "uuid": "^8.0.0"
  },
  "bugs": {
    "url": "https://github.com/luyuhuang/vscode-rss/issues"
  }
}
